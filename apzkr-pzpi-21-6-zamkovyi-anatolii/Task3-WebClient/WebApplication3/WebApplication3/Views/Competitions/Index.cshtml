@model IEnumerable<WebApplication3.Models.Competition>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OrganizationId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CoachId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompetitionName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompetitionCountry)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompetitionCity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RegistrationDeadline)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CompetitioneDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OrganizationId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CoachId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompetitionName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompetitionCountry)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompetitionCity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RegistrationDeadline)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompetitioneDate)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CompetitionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CompetitionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CompetitionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
